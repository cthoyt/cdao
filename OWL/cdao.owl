    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#DesoxiRibonucleotideResidue -->

    <owl:Class rdf:about="#DesoxiRibonucleotideResidue">
        <rdfs:subClassOf rdf:resource="#Molecular"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#DesoxiRibonucleotideResidueCharacter -->

    <owl:Class rdf:about="#DesoxiRibonucleotideResidueCharacter">
        <rdfs:subClassOf rdf:resource="#MolecularCharacter"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Datum"/>
                <owl:allValuesFrom rdf:resource="#DesoxiRibonucleotideResidueStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Nucleotide_Datum"/>
                <owl:someValuesFrom rdf:resource="#DesoxiRibonucleotideResidueStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#DesoxiRibonucleotideResidueStateDatum -->

    <owl:Class rdf:about="#DesoxiRibonucleotideResidueStateDatum">
        <rdfs:subClassOf rdf:resource="#MolecularStateDatum"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#DirectedEdge -->


    <owl:Class rdf:about="#DirectedEdge">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Edge"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#has_Child_Node"/>
                        <owl2:onClass rdf:resource="#Node"/>
                        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#has_Parent_Node"/>
                        <owl2:onClass rdf:resource="#Node"/>
                        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <dc:description
            >A directed edge. Rooted trees have directed edges. The direction is specified by way of the parent and child relationships of nodes that the edge connects.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#DistanceMatrixBasedMethod -->

    <owl:Class rdf:about="#DistanceMatrixBasedMethod">
        <rdfs:subClassOf rdf:resource="#TreeProcedure"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Edge -->

    <owl:Class rdf:about="#Edge">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Node"/>
                <owl2:onClass rdf:resource="#Node"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Annotation"/>
                <owl:someValuesFrom rdf:resource="#EdgeAnnotation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >An edge connecting two nodes in a (Phylogenetic) Tree or Network, also known as a &#39;branch&#39;.  Edges may have attributes such as length, degree of support, and direction.  An edge can be a surrogate for a &#39;split&#39; or bipartition, since each edge in a tree divides the terminal nodes into two sets.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#EdgeAnnotation -->

    <owl:Class rdf:about="#EdgeAnnotation">
        <rdfs:subClassOf rdf:resource="#CDAOAnnotation"/>
        <dc:description
            >Additional information that can be attached to a single edge</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#EdgeLength -->

    <owl:Class rdf:about="#EdgeLength">
        <rdfs:subClassOf rdf:resource="#EdgeAnnotation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Value"/>
                <owl:someValuesFrom rdf:resource="&xsd;anySimpleType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >The length of an edge (branch) of a Tree or Network, typically in units of evolutionary changes in character-state per character.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#EdgeTransition -->

    <owl:Class rdf:about="#EdgeTransition">
        <rdfs:subClassOf rdf:resource="#EdgeAnnotation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#transition_of"/>
                <owl2:onClass rdf:resource="#Character"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Left_State"/>
                <owl2:onClass rdf:resource="#CharacterStateDomain"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Right_State"/>
                <owl2:onClass rdf:resource="#CharacterStateDomain"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment
            >If left state is same as left node, then transition will imply correct direction.</rdfs:comment>
        <dc:description
            >A transition (in states of a character) associated with an edge (branch) of a Tree or Network.
Each transition has a left state (original) and a right state (resutl of the transition).</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#GammaDistribution -->

    <owl:Class rdf:about="#GammaDistribution">
        <rdfs:subClassOf rdf:resource="#SubstitutionModel"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#GapCost -->

    <owl:Class rdf:about="#GapCost">
        <rdfs:subClassOf rdf:resource="#ModelDescription"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Generic_State -->

    <owl:Class rdf:about="#Generic_State">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#absent"/>
                    <rdf:Description rdf:about="#unknown"/>
                    <rdf:Description rdf:about="#gap"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="#Categorical"/>
        <dc:description
            >This concept is tied to the verbally ambiguous &#39;gap&#39; concept and to the use of a gap character (often the en dash &#39;-&#39;) in text representations of sequence alignments. In general, this represents the absence of any positively diagnosed Character-State. As such, the gap may be interpreted as an additional Character-State, as the absence of the Character, or as an unknown value.  In some cases it is helpful to separate these.</dc:description>
        <rdfs:comment
            >This class should be renamed.  These are not generic states but non-concrete states including gap, unknown and missing.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#JukesKantor -->

    <owl:Class rdf:about="#JukesKantor">
        <rdfs:subClassOf rdf:resource="#SubstitutionModel"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Kimura2Parameters -->

    <owl:Class rdf:about="#Kimura2Parameters">
        <rdfs:subClassOf rdf:resource="#SubstitutionModel"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Lineage -->

    <owl:Class rdf:about="#Lineage">
        <rdfs:subClassOf rdf:resource="#Tree"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Lineage_node"/>
                <owl:someValuesFrom rdf:resource="#Node"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >A path in the tree representing the ancestor of a particular TU</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#MRCANode -->

    <owl:Class rdf:about="#MRCANode">
        <rdfs:subClassOf rdf:resource="#CommonAncestralNode"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#nca_node_of"/>
                <owl2:onClass rdf:resource="#SetOfThings"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >The most recent common ancestor of a collection of nodes in the tree</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#MaximumLikelihoodMethod -->

    <owl:Class rdf:about="#MaximumLikelihoodMethod">
        <rdfs:subClassOf rdf:resource="#TreeProcedure"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#ModelDescription -->

    <owl:Class rdf:about="#ModelDescription">
        <rdfs:subClassOf rdf:resource="#CDAOAnnotation"/>
        <dc:description
            >Description of a model of transitions.</dc:description>
        <rdfs:comment
            >This is a non-computible description of a model, not the fully specified mathematical model, which typically relates the probability of a transition to various parameters.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Molecular -->

    <owl:Class rdf:about="#Molecular">
        <rdfs:subClassOf rdf:resource="#Categorical"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#MolecularCharacter -->

    <owl:Class rdf:about="#MolecularCharacter">
        <rdfs:subClassOf rdf:resource="#CategoricalCharacter"/>
        <dc:description
            >A character described by a single residue</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#MolecularStateDatum -->

    <owl:Class rdf:about="#MolecularStateDatum">
        <rdfs:subClassOf rdf:resource="#CategoricalStateDatum"/>
        <dc:description
            >Instance of a character for a given TU, in the case of a molecular character</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Network -->

    <owl:Class rdf:about="#Network">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#Edge"/>
                            <rdf:Description rdf:about="#Node"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >A kind of phylogenetic tree in which some typical restrictions are not satisfied (e.g., presence of nodes with multiple parents)</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Node -->

    <owl:Class rdf:about="#Node">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#part_of"/>
                <owl2:onClass rdf:resource="#Network"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#belongs_to_Edge_as_Child"/>
                <owl2:onClass rdf:resource="#DirectedEdge"/>
                <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >Class describing a node in a tree</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#ParsimonyMethod -->

    <owl:Class rdf:about="#ParsimonyMethod">
        <rdfs:subClassOf rdf:resource="#TreeProcedure"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#PolymorphicStateDomain -->

    <owl:Class rdf:about="#PolymorphicStateDomain">
        <rdfs:subClassOf rdf:resource="#CharacterStateDomain"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Uncertainty_Factor"/>
                <owl:hasValue rdf:datatype="&xsd;int">1</owl:hasValue>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#RibonucleotideResidue -->

    <owl:Class rdf:about="#RibonucleotideResidue">
        <rdfs:subClassOf rdf:resource="#Molecular"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#RibonucleotideResidueCharacter -->

    <owl:Class rdf:about="#RibonucleotideResidueCharacter">
        <rdfs:subClassOf rdf:resource="#MolecularCharacter"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Datum"/>
