    <!-- http://www.co-ode.org/ontologies/amino-acid/2005/10/11/amino-acid.owl#AminoAcid -->

    <owl:Class rdf:about="http://www.co-ode.org/ontologies/amino-acid/2005/10/11/amino-acid.owl#AminoAcid">
        <owl:equivalentClass rdf:resource="#AminoAcidResidue"/>
        <dc:date>09/15/2008</dc:date>
        <dc:description
            >This class describes a single amino acid residue; the concrete definition is imported from the external ontology at www.co-ode.org/ontologies/amino-acid.</dc:description>
        <dc:creator>CDAO-team</dc:creator>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#AlignmentProcedure -->

    <owl:Class rdf:about="#AlignmentProcedure">
        <rdfs:subClassOf rdf:resource="#CharacterStateDataMatrixAnnotation"/>
        <dc:description
            >Algorithm employed to derive the character state data matrix</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#AminoAcidResidue -->

    <owl:Class rdf:about="#AminoAcidResidue">
        <rdfs:subClassOf rdf:resource="#Molecular"/>
        <dc:description
            >This class will be declared equivalent ot the amino acid class description imported</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#AminoAcidResidueCharacter -->

