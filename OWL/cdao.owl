                <owl:onProperty rdf:resource="#has_Amino_Acid_Datum"/>
                <owl:someValuesFrom rdf:resource="#AminoAcidResidueStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#AminoAcidResidueStateDatum -->

    <owl:Class rdf:about="#AminoAcidResidueStateDatum">
        <rdfs:subClassOf rdf:resource="#MolecularStateDatum"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#AncestralNode -->

    <owl:Class rdf:about="#AncestralNode">
        <rdfs:subClassOf rdf:resource="#Node"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#part_of"/>
                <owl2:onClass rdf:resource="#RootedTree"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >A node that has descendants in the tree</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#BayesianMethod -->

    <owl:Class rdf:about="#BayesianMethod">
        <rdfs:subClassOf rdf:resource="#TreeProcedure"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Branch -->

    <owl:Class rdf:about="#Branch">
        <owl:equivalentClass rdf:resource="#Edge"/>
        <dc:description
            >&#39;Branch&#39; is the domain-specific synonym for an edge of a (Phylogenetic) Tree or Network.  Branches may have properties such as length and degree of support.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CDAOAnnotation -->

    <owl:Class rdf:about="#CDAOAnnotation">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <dc:description
            >The base class of annotations in CDAO.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Categorical -->

    <owl:Class rdf:about="#Categorical">
        <rdfs:subClassOf rdf:resource="#CharacterStateDomain"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CategoricalCharacter -->

    <owl:Class rdf:about="#CategoricalCharacter">
        <rdfs:subClassOf rdf:resource="#Character"/>
        <dc:description
            >Class describing a discrete character</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CategoricalStateDatum -->

    <owl:Class rdf:about="#CategoricalStateDatum">
        <rdfs:subClassOf rdf:resource="#CharacterStateDatum"/>
        <dc:description
            >Instance of a character for a given TU in the case of a discrete character</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Character -->

    <owl:Class rdf:about="#Character">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Datum"/>
                <owl:someValuesFrom rdf:resource="#CharacterStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >Traits shown to be relevant for phylogenetic classification</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CharacterStateDataMatrix -->

    <owl:Class rdf:about="#CharacterStateDataMatrix">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has"/>
                <owl:someValuesFrom rdf:resource="#CharacterStateDataMatrixAnnotation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_TU"/>
                <owl:someValuesFrom rdf:resource="#TU"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Character"/>
                <owl:someValuesFrom rdf:resource="#Character"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >A matrix of character-state data, typically containing observed data, though in some cases the states in the matrix might be simulated or hypothetical. Synonyms: character Data matrix, character-state matrix</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CharacterStateDataMatrixAnnotation -->

    <owl:Class rdf:about="#CharacterStateDataMatrixAnnotation">
        <rdfs:subClassOf rdf:resource="#CDAOAnnotation"/>
        <dc:description
            >Meta-information associated with a character matrix, such as, for the case of a sequence alignment, the method of alignment.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CharacterStateDatum -->

    <owl:Class rdf:about="#CharacterStateDatum">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#belongs_to_Character"/>
                <owl2:onClass rdf:resource="#Character"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#belongs_to_TU"/>
                <owl2:onClass rdf:resource="#TU"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >The instance of a given character for a given TU.  Its state is an object property drawn from a particular character state domain, e.g., the state of an Amino_Acid_State_Datum is an object property drawn from the domain Amino_Acid.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CharacterStateDomain -->

    <owl:Class rdf:about="#CharacterStateDomain">
        <dc:description
            >The universe of possible states for a particular type of character, e.g., the states of an Amino_Acid character come from the Amino_Acid domain.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CommonAncestralNode -->

    <owl:Class rdf:about="#CommonAncestralNode">
        <rdfs:subClassOf rdf:resource="#AncestralNode"/>
        <dc:description
            >A node in the tree that is the ancestor of  descendants along different branches</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Compound -->

    <owl:Class rdf:about="#Compound">
        <rdfs:subClassOf rdf:resource="#Categorical"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CompoundCharacter -->

    <owl:Class rdf:about="#CompoundCharacter">
        <rdfs:subClassOf rdf:resource="#CategoricalCharacter"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Compound_Datum"/>
                <owl:someValuesFrom rdf:resource="#CompoundStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Datum"/>
                <owl:allValuesFrom rdf:resource="#CompoundStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >A character that could be divided into separate characters but is not due to the non-independence of changes that would result, e.g., as in the case of a subsequence that is either present or absent as a block.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CompoundStateDatum -->

    <owl:Class rdf:about="#CompoundStateDatum">
        <rdfs:subClassOf rdf:resource="#CategoricalStateDatum"/>
        <dc:description
            >Instance of a character for a given TU, in the case of a compound type of character</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Continuous -->

    <owl:Class rdf:about="#Continuous">
        <rdfs:subClassOf rdf:resource="#CharacterStateDomain"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Value"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >This class describes a continuous value. The link to the actual float value is through the property has_Value. It could have also other properties attached (e.g., has_Precision).</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#ContinuousCharacter -->

    <owl:Class rdf:about="#ContinuousCharacter">
        <rdfs:subClassOf rdf:resource="#Character"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Datum"/>
                <owl:allValuesFrom rdf:resource="#ContinuousStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Continuous_Datum"/>
                <owl:someValuesFrom rdf:resource="#ContinuousStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >A character described by a continuous value</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#ContinuousStateDatum -->

    <owl:Class rdf:about="#ContinuousStateDatum">
        <rdfs:subClassOf rdf:resource="#CharacterStateDatum"/>
        <dc:description
            >Instance of a character for a TU, in the case of a continuous character</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CoordinateList -->

    <owl:Class rdf:about="#CoordinateList">
        <rdfs:subClassOf rdf:resource="#DatumCoordinate"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CoordinatePoint -->

    <owl:Class rdf:about="#CoordinatePoint">
        <rdfs:subClassOf rdf:resource="#DatumCoordinate"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CoordinateRange -->

    <owl:Class rdf:about="#CoordinateRange">
        <rdfs:subClassOf rdf:resource="#DatumCoordinate"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#CoordinateSystem -->

    <owl:Class rdf:about="#CoordinateSystem">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <dc:description
            >A reference to an external coordinate system.  Coordinates for data must refer to some such external coordinate system.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#DatumCoordinate -->

    <owl:Class rdf:about="#DatumCoordinate">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Coordinate_System"/>
                <owl2:onClass rdf:resource="#CoordinateSystem"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >A positional coordinate giving the source of a character state, used for molecular sequences.</dc:description>
