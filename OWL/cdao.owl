<?xml version="1.0"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY owl2 "http://www.w3.org/2006/12/owl2#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl2xml "http://www.w3.org/2006/12/owl2-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY cdao "http://www.evolutionaryontology.org/cdao.owl#" >
]>


<rdf:RDF xmlns="http://www.evolutionaryontology.org/cdao.owl#"
     xml:base="http://www.evolutionaryontology.org/cdao.owl"
     xmlns:owl2xml="http://www.w3.org/2006/12/owl2-xml#"
     xmlns:cdao="http://www.evolutionaryontology.org/cdao.owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl2="http://www.w3.org/2006/12/owl2#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#">
    <owl:Ontology rdf:about="">
        <owl:imports rdf:resource="http://www.co-ode.org/ontologies/amino-acid/2005/10/11/amino-acid.owl"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:AnnotationProperty rdf:about="&dc;language"/>
    <owl:AnnotationProperty rdf:about="&dc;description"/>
    <owl:AnnotationProperty rdf:about="&dc;subject"/>
    <owl:AnnotationProperty rdf:about="&dc;coverage"/>
    <owl:AnnotationProperty rdf:about="&dc;source"/>
    <owl:AnnotationProperty rdf:about="&dc;creator"/>
    <owl:AnnotationProperty rdf:about="&dc;date"/>
    <owl:AnnotationProperty rdf:about="&dc;title"/>
    <owl:AnnotationProperty rdf:about="&dc;rights"/>
    <owl:AnnotationProperty rdf:about="&dc;identifier"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to -->

    <owl:ObjectProperty rdf:about="#belongs_to">
        <dc:description
            >Generic property that links a concept to another concept it is a constituent of. The property is a synonym of part_of.</dc:description>
        <owl:equivalentProperty rdf:resource="#part_of"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Amino_Acid_Character -->

    <owl:ObjectProperty rdf:about="#belongs_to_Amino_Acid_Character">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#AminoAcidResidueCharacter"/>
        <rdfs:domain rdf:resource="#AminoAcidResidueStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to_Character"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Character -->

    <owl:ObjectProperty rdf:about="#belongs_to_Character">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#Character"/>
        <rdfs:domain rdf:resource="#CharacterStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Character_State_Data_Matrix -->

    <owl:ObjectProperty rdf:about="#belongs_to_Character_State_Data_Matrix">
        <rdfs:range rdf:resource="#CharacterStateDataMatrix"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Character"/>
                    <rdf:Description rdf:about="#TU"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Compound_Character -->

    <owl:ObjectProperty rdf:about="#belongs_to_Compound_Character">
        <rdfs:range rdf:resource="#CompoundCharacter"/>
        <rdfs:domain rdf:resource="#CompoundStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to_Character"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Continuous_Character -->

    <owl:ObjectProperty rdf:about="#belongs_to_Continuous_Character">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#ContinuousCharacter"/>
        <rdfs:domain rdf:resource="#ContinuousStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to_Character"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Edge -->

    <owl:ObjectProperty rdf:about="#belongs_to_Edge">
        <dc:description
            >The property links a Node to one of the edges that are incident on such node.</dc:description>
        <rdfs:range rdf:resource="#Edge"/>
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Edge_as_Child -->

    <owl:ObjectProperty rdf:about="#belongs_to_Edge_as_Child">
        <dc:description
            >The property links a Node to the Edge it belongs to in the child position.</dc:description>
        <rdfs:range rdf:resource="#DirectedEdge"/>
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to_Edge"/>
        <owl:inverseOf rdf:resource="#has_Child_Node"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Edge_as_Parent -->

    <owl:ObjectProperty rdf:about="#belongs_to_Edge_as_Parent">
        <dc:description
            >The property links a Node to one of the Edges where the node appears in the parent position (i.e., closer to the root).</dc:description>
        <rdfs:range rdf:resource="#DirectedEdge"/>
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to_Edge"/>
        <owl:inverseOf rdf:resource="#has_Parent_Node"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Network -->

    <owl:ObjectProperty rdf:about="#belongs_to_Network">
        <rdfs:range rdf:resource="#Network"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Edge"/>
                    <rdf:Description rdf:about="#Node"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Nucleotide_Character -->

    <owl:ObjectProperty rdf:about="#belongs_to_Nucleotide_Character">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:range rdf:resource="#DesoxiRibonucleotideResidueCharacter"/>
        <rdfs:domain rdf:resource="#DesoxiRibonucleotideResidueStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to_Character"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_TU -->

    <owl:ObjectProperty rdf:about="#belongs_to_TU">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a character-state datum to its TU.</dc:description>
        <rdfs:domain rdf:resource="#CharacterStateDatum"/>
        <rdfs:range rdf:resource="#TU"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Tree -->

    <owl:ObjectProperty rdf:about="#belongs_to_Tree">
        <rdfs:range rdf:resource="#Tree"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Edge"/>
                    <rdf:Description rdf:about="#Node"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#belongs_to_Tree_as_Root -->

    <owl:ObjectProperty rdf:about="#belongs_to_Tree_as_Root">
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:range rdf:resource="#Tree"/>
        <rdfs:subPropertyOf rdf:resource="#belongs_to"/>
        <owl:inverseOf rdf:resource="#has_Root"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#connects_to -->

    <owl:ObjectProperty rdf:about="#connects_to">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <owl:inverseOf rdf:resource="#connects_to"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has -->

    <owl:ObjectProperty rdf:about="#has">
        <dc:description
            >Generic &#39;has&#39; property.</dc:description>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Amino_Acid_Datum -->

    <owl:ObjectProperty rdf:about="#has_Amino_Acid_Datum">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates an amino acid character (a column in a protein sequence alignment) to a state datum for the character (an individual cell in the alignment column).</dc:description>
        <rdfs:range rdf:resource="#AminoAcidResidueStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has_Molecular_Datum"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#AminoAcidResidueCharacter"/>
                    <rdf:Description rdf:about="#TU"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Amino_Acid_State -->

    <owl:ObjectProperty rdf:about="#has_Amino_Acid_State">
        <dc:description rdf:datatype="&xsd;string"
            >This property associates a amino acid character-state instance with a state value from the domain of amino acid states.</dc:description>
        <rdfs:domain rdf:resource="#AminoAcidResidueStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has_State"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#AminoAcidResidue"/>
                    <rdf:Description rdf:about="#Generic_State"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Ancestor -->

    <owl:ObjectProperty rdf:about="#has_Ancestor">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <dc:description
            >The property links a node to any of the other nodes that are its ancestors in a rooted tree.</dc:description>
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
        <owl:inverseOf rdf:resource="#has_Descendant"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Annotation -->

    <owl:ObjectProperty rdf:about="#has_Annotation">
        <rdfs:range rdf:resource="#CDAOAnnotation"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
        <owl:inverseOf rdf:resource="#is_annotation_of"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Categorical_Datum -->

    <owl:ObjectProperty rdf:about="#has_Categorical_Datum">
        <rdfs:subPropertyOf rdf:resource="#has_Datum"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Character -->

    <owl:ObjectProperty rdf:about="#has_Character">
        <dc:description rdf:datatype="&xsd;string"
            >This property associates a character data matrix with a character (a column) represented in the matrix.</dc:description>
        <rdfs:range rdf:resource="#Character"/>
        <rdfs:domain rdf:resource="#CharacterStateDataMatrix"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Child -->

    <owl:ObjectProperty rdf:about="#has_Child">
        <dc:description
            >The property links a node to a node that is an immediate descendant in the tree.</dc:description>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
        <owl:inverseOf rdf:resource="#has_Parent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Child_Node -->

    <owl:ObjectProperty rdf:about="#has_Child_Node">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:description
            >The property associates to a Directed Edge the Node that is in the child position in the edge, i.e., the node touched by the edge and closer to the leaves of the tree.</dc:description>
        <rdfs:domain rdf:resource="#DirectedEdge"/>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#has_Node"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Compound_Datum -->

    <owl:ObjectProperty rdf:about="#has_Compound_Datum">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a compound character (a character with some states that are subdividable) to a state datum for the character.</dc:description>
        <rdfs:range rdf:resource="#CompoundStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has_Categorical_Datum"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#CompoundCharacter"/>
                    <rdf:Description rdf:about="#TU"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Compound_State -->

    <owl:ObjectProperty rdf:about="#has_Compound_State">
        <dc:description rdf:datatype="&xsd;string"
            >This property associates a compound character-state instance with its compound state value.</dc:description>
        <rdfs:domain rdf:resource="#CompoundStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has_State"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Compound"/>
                    <rdf:Description rdf:about="#Generic_State"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Continuous_Datum -->

    <owl:ObjectProperty rdf:about="#has_Continuous_Datum">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a continuous character to a state datum for the character.</dc:description>
        <rdfs:range rdf:resource="#ContinuousStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has_Datum"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#ContinuousCharacter"/>
                    <rdf:Description rdf:about="#TU"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Continuous_State -->

    <owl:ObjectProperty rdf:about="#has_Continuous_State">
        <dc:description rdf:datatype="&xsd;string"
            >This property associates a character-state instance with a state value on a continuous numeric scale.</dc:description>
        <rdfs:range rdf:resource="#Continuous"/>
        <rdfs:domain rdf:resource="#ContinuousStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has_State"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Coordinate -->

    <owl:ObjectProperty rdf:about="#has_Coordinate">
        <rdfs:domain rdf:resource="#CharacterStateDatum"/>
        <rdfs:range rdf:resource="#DatumCoordinate"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Coordinate_System -->

    <owl:ObjectProperty rdf:about="#has_Coordinate_System">
        <dc:description rdf:datatype="&xsd;string"
            >This property links a coordinate to the coordinate system it references.</dc:description>
        <rdfs:range rdf:resource="#CoordinateSystem"/>
        <rdfs:domain rdf:resource="#DatumCoordinate"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Datum -->

    <owl:ObjectProperty rdf:about="#has_Datum">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a character to a state datum for the character.</dc:description>
        <rdfs:range rdf:resource="#CharacterStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Character"/>
                    <rdf:Description rdf:about="#TU"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Descendant -->

    <owl:ObjectProperty rdf:about="#has_Descendant">
        <rdf:type rdf:resource="&owl;TransitiveProperty"/>
        <dc:description
            >A property that links a node to any of its descendants in a rooted tree.</dc:description>
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Element -->

    <owl:ObjectProperty rdf:about="#has_Element">
        <rdfs:domain rdf:resource="#SetOfThings"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_First_Coordinate_Item -->

    <owl:ObjectProperty rdf:about="#has_First_Coordinate_Item">
        <dc:description rdf:datatype="&xsd;string"
            >The property that relates a coordinate list to the first item in the list.</dc:description>
        <rdfs:domain rdf:resource="#CoordinateList"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#CoordinatePoint"/>
                    <rdf:Description rdf:about="#CoordinateRange"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Left_Node -->

    <owl:ObjectProperty rdf:about="#has_Left_Node">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a transition to a &#39;left&#39; node (the node that has the &#39;left&#39; state).</dc:description>
        <rdfs:domain rdf:resource="#EdgeTransition"/>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#has_Transition_Component"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Left_State -->

    <owl:ObjectProperty rdf:about="#has_Left_State">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a transition to a &#39;left&#39; state (the state associated with the &#39;left&#39; node).</dc:description>
        <rdfs:range rdf:resource="#CharacterStateDomain"/>
        <rdfs:domain rdf:resource="#EdgeTransition"/>
        <rdfs:subPropertyOf rdf:resource="#has_Transition_Component"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Lineage_node -->

    <owl:ObjectProperty rdf:about="#has_Lineage_node">
        <rdfs:domain rdf:resource="#Lineage"/>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Molecular_Datum -->

    <owl:ObjectProperty rdf:about="#has_Molecular_Datum">
        <rdfs:range rdf:resource="#MolecularStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has_Categorical_Datum"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#MolecularCharacter"/>
                    <rdf:Description rdf:about="#TU"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Node -->

    <owl:ObjectProperty rdf:about="#has_Node">
        <dc:description
            >Property that associates to each Edge the Nodes it connects.</dc:description>
        <rdfs:domain rdf:resource="#Edge"/>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Nucleotide_Datum -->

    <owl:ObjectProperty rdf:about="#has_Nucleotide_Datum">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a nucleotide character (a column in a nucleotide alignment) to a state datum for the character (an individual cell in the alignment column).</dc:description>
        <rdfs:range rdf:resource="#DesoxiRibonucleotideResidueStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has_Molecular_Datum"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#DesoxiRibonucleotideResidueCharacter"/>
                    <rdf:Description rdf:about="#TU"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Nucleotide_State -->

    <owl:ObjectProperty rdf:about="#has_Nucleotide_State">
        <dc:description rdf:datatype="&xsd;string"
            >This property associates a nucleotide character-state instance with a state value from the domain of nucleotide states.</dc:description>
        <rdfs:domain rdf:resource="#DesoxiRibonucleotideResidueStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has_State"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#DesoxiRibonucleotideResidue"/>
                    <rdf:Description rdf:about="#Generic_State"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Parent -->

    <owl:ObjectProperty rdf:about="#has_Parent">
        <dc:description
            >The property that links a node to its unique parent in a rooted tree.</dc:description>
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Parent_Node -->

    <owl:ObjectProperty rdf:about="#has_Parent_Node">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:description
            >Associates to a Directed Edge the Node that is in the parent position in the edge (i.e., the node touched by the edge and closer to the root of the tree)</dc:description>
        <rdfs:domain rdf:resource="#DirectedEdge"/>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#has_Node"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_RNA_Datum -->

    <owl:ObjectProperty rdf:about="#has_RNA_Datum">
        <rdfs:subPropertyOf rdf:resource="#has_Molecular_Datum"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Remaining_Coordinate_List -->

    <owl:ObjectProperty rdf:about="#has_Remaining_Coordinate_List">
        <dc:description rdf:datatype="&xsd;string"
            >The property that relates a coordinate list to the item in the list beyond the first item.</dc:description>
        <rdfs:range rdf:resource="#CoordinateList"/>
        <rdfs:domain rdf:resource="#CoordinateList"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Right_Node -->

    <owl:ObjectProperty rdf:about="#has_Right_Node">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a transition to a &#39;right&#39; node (the node that has the &#39;right&#39; state).</dc:description>
        <rdfs:domain rdf:resource="#EdgeTransition"/>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:subPropertyOf rdf:resource="#has_Transition_Component"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Right_State -->

    <owl:ObjectProperty rdf:about="#has_Right_State">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a transition to a &#39;right&#39; state (the state associated with the &#39;right&#39; node).</dc:description>
        <rdfs:range rdf:resource="#CharacterStateDomain"/>
        <rdfs:domain rdf:resource="#EdgeTransition"/>
        <rdfs:subPropertyOf rdf:resource="#has_Transition_Component"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Root -->

    <owl:ObjectProperty rdf:about="#has_Root">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:description
            >The property links a rooted tree to the specific node that represents the unique root of the tree.</dc:description>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:domain rdf:resource="#RootedTree"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Standard_Datum -->

    <owl:ObjectProperty rdf:about="#has_Standard_Datum">
        <rdfs:range rdf:resource="#StandardStateDatum"/>
        <rdfs:subPropertyOf rdf:resource="#has_Categorical_Datum"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#StandardCharacter"/>
                    <rdf:Description rdf:about="#TU"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_State -->

    <owl:ObjectProperty rdf:about="#has_State">
        <dc:description rdf:datatype="&xsd;string"
            >This property associates a character-state instance with its state value, e.g., a state value expressed in terms of an imported domain ontology.</dc:description>
        <rdfs:domain rdf:resource="#CharacterStateDatum"/>
        <rdfs:range rdf:resource="#CharacterStateDomain"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_TU -->

    <owl:ObjectProperty rdf:about="#has_TU">
        <dc:description rdf:datatype="&xsd;string"
            >This property associates a character data matrix with a TU (a row) represented in the matrix.</dc:description>
        <rdfs:domain rdf:resource="#CharacterStateDataMatrix"/>
        <rdfs:range rdf:resource="#TU"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#has_Transition_Component -->

    <owl:ObjectProperty rdf:about="#has_Transition_Component">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a transition to the components that compose it.</dc:description>
        <rdfs:domain rdf:resource="#EdgeTransition"/>
        <rdfs:subPropertyOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#homologous_to -->

    <owl:ObjectProperty rdf:about="#homologous_to">
        <dc:description rdf:datatype="&xsd;string"
            >This propery relates different instances of the same character, including the case when the states of the character differ (e.g., large_beak of beak_size_character of TU A is homologous_to small_beak of beak_size_character of TU B).</dc:description>
        <rdfs:domain rdf:resource="#CharacterStateDatum"/>
        <rdfs:range rdf:resource="#CharacterStateDatum"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#is_annotation_of -->

    <owl:ObjectProperty rdf:about="#is_annotation_of">
        <rdfs:domain rdf:resource="#CDAOAnnotation"/>
        <rdfs:range rdf:resource="&owl;Thing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#nca_node_of -->

    <owl:ObjectProperty rdf:about="#nca_node_of">
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:range rdf:resource="#SetOfNodes"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#part_of -->

    <owl:ObjectProperty rdf:about="#part_of">
        <owl:inverseOf rdf:resource="#has"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#precedes -->

    <owl:ObjectProperty rdf:about="#precedes"/>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#represented_by_Node -->

    <owl:ObjectProperty rdf:about="#represented_by_Node">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a TU to a node that represents it in a network.</dc:description>
        <rdfs:range rdf:resource="#Node"/>
        <rdfs:domain rdf:resource="#TU"/>
        <owl:inverseOf rdf:resource="#represents_TU"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#represents_TU -->

    <owl:ObjectProperty rdf:about="#represents_TU">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a TU or taxonomic unit (typically associated with character data) to a phylogenetic history (Tree).</dc:description>
        <rdfs:domain rdf:resource="#Node"/>
        <rdfs:range rdf:resource="#TU"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#subtree_of -->

    <owl:ObjectProperty rdf:about="#subtree_of">
        <dc:description
            >This property links two networks where the latter is a substructure of the former</dc:description>
        <rdfs:range rdf:resource="#Network"/>
        <rdfs:domain rdf:resource="#Network"/>
    </owl:ObjectProperty>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#transition_of -->

    <owl:ObjectProperty rdf:about="#transition_of">
        <dc:description rdf:datatype="&xsd;string"
            >This property relates a type of evolutionary change (an Edge_Transition) to the character that undergoes the change.  The change is a transition_of the affected character.</dc:description>
        <rdfs:range rdf:resource="#Character"/>
        <rdfs:domain rdf:resource="#EdgeTransition"/>
    </owl:ObjectProperty>
    


