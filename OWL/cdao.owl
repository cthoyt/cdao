    <owl:Class rdf:about="#TreeProcedure">
        <rdfs:subClassOf rdf:resource="#TreeAnnotation"/>
        <dc:description
            >Algorithm used to develop the tree</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#UncertainStateDomain -->

    <owl:Class rdf:about="#UncertainStateDomain">
