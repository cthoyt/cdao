            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#RibonucleotideResidueStateDatum -->

    <owl:Class rdf:about="#RibonucleotideResidueStateDatum">
        <rdfs:subClassOf rdf:resource="#MolecularStateDatum"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#RootedTree -->

    <owl:Class rdf:about="#RootedTree">
        <rdfs:subClassOf rdf:resource="#Tree"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#DirectedEdge"/>
                            <rdf:Description rdf:about="#Node"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Root"/>
                <owl2:onClass rdf:resource="#Node"/>
                <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#subtree_of"/>
                <owl:allValuesFrom rdf:resource="#RootedTree"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#UnrootedTree"/>
        <dc:description
            >A tree where the root is known</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Sequence -->

    <owl:Class rdf:about="#Sequence">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has"/>
                <owl:someValuesFrom rdf:resource="#CharacterStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has"/>
                <owl:allValuesFrom>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#has_Coordinate"/>
                        <owl2:onClass rdf:resource="#DatumCoordinate"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
                    </owl:Restriction>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >A set of ordered states, typically the residues in a macromolecular sequence.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#SetOfCharacters -->

    <owl:Class rdf:about="#SetOfCharacters">
        <rdfs:subClassOf rdf:resource="#SetOfThings"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#SetOfNodes -->

    <owl:Class rdf:about="#SetOfNodes">
        <rdfs:subClassOf rdf:resource="#SetOfThings"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Element"/>
                <owl:allValuesFrom rdf:resource="#Node"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#SetOfThings -->

    <owl:Class rdf:about="#SetOfThings">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Element"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="#Character"/>
                            <rdf:Description rdf:about="#SetOfCharacters"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >The class is used to describe either colletions of characters or higher order grouping (e.g., groups of groups of characters). This extends the CharSet block of NEXUS.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#Standard -->

    <owl:Class rdf:about="#Standard">
        <rdfs:subClassOf rdf:resource="#Categorical"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#StandardCharacter -->

    <owl:Class rdf:about="#StandardCharacter">
        <rdfs:subClassOf rdf:resource="#CategoricalCharacter"/>
        <dc:description
            >A generic discrete character</dc:description>
    </owl:Class>
    


