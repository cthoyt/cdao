    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#SubstitutionModel -->

    <owl:Class rdf:about="#SubstitutionModel">
        <rdfs:subClassOf rdf:resource="#ModelDescription"/>
    </owl:Class>
    


