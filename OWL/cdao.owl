    <!-- http://www.evolutionaryontology.org/cdao.owl#Tree -->

    <owl:Class rdf:about="#Tree">
        <rdfs:subClassOf rdf:resource="#Network"/>
        <dc:description
            >A c Tree represents evolutionary paths of descent-with-modification from common ancestors. Typically a  Tree is assumed to be a connected, acyclic graph in which Nodes have no more than one parent</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdao.owl#TreeAnnotation -->

    <owl:Class rdf:about="#TreeAnnotation">
