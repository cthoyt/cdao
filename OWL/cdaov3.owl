    <!-- http://www.evolutionaryontology.org/cdaov3.owl#has_Range_Start_Value -->

    <owl:DatatypeProperty rdf:about="#has_Range_Start_Value">
        <rdfs:domain rdf:resource="#CoordinateRange"/>
        <rdfs:range rdf:resource="&xsd;integer"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#has_Support_Value -->

    <owl:DatatypeProperty rdf:about="#has_Support_Value">
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#has_Uncertainty_Factor -->

    <owl:DatatypeProperty rdf:about="#has_Uncertainty_Factor">
        <rdfs:range rdf:resource="&xsd;float"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#has_Value -->

    <owl:DatatypeProperty rdf:about="#has_Value">
        <rdfs:range rdf:resource="&xsd;anySimpleType"/>
    </owl:DatatypeProperty>
    


