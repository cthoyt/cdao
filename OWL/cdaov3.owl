    <!-- http://purl.org/obo/owl/mao.obo#MAO_0000000 -->

    <owl:Class rdf:about="&mao;MAO_0000000">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#belongs_to"/>
                <owl:someValuesFrom rdf:resource="#CharacterStateDataMatrix"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://purl.org/obo/owl/mao.obo#MAO_0000037 -->

    <owl:Class rdf:about="&mao;MAO_0000037">
        <rdfs:subClassOf rdf:resource="#Sequence"/>
    </owl:Class>
    


    <!-- http://purl.org/obo/owl/mao.obo#MAO_0000084 -->

    <owl:Class rdf:about="&mao;MAO_0000084">
        <rdfs:subClassOf rdf:resource="#Character"/>
    </owl:Class>
    


    <!-- http://purl.org/obo/owl/mao.obo#MAO_0000120 -->

    <owl:Class rdf:about="&mao;MAO_0000120">
        <owl:equivalentClass rdf:resource="#AminoAcidResidueStateDatum"/>
    </owl:Class>
    


    <!-- http://purl.org/obo/owl/mao.obo#MAO_0000121 -->

    <owl:Class rdf:about="&mao;MAO_0000121">
        <owl:equivalentClass rdf:resource="#DesoxiRibonucleotideResidueStateDatum"/>
    </owl:Class>
    


    <!-- http://purl.org/obo/owl/mao.obo#MAO_0000126 -->

    <owl:Class rdf:about="&mao;MAO_0000126">
        <rdfs:subClassOf rdf:resource="#geneticEvent"/>
    </owl:Class>
    


    <!-- http://www.co-ode.org/ontologies/amino-acid/2005/10/11/amino-acid.owl#AminoAcid -->

    <owl:Class rdf:about="&amino-acid;AminoAcid">
        <owl:equivalentClass rdf:resource="#AminoAcidResidue"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#AlignmentProcedure -->

    <owl:Class rdf:about="#AlignmentProcedure">
        <rdfs:subClassOf rdf:resource="#CharacterStateDataMatrixAnnotation"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#AminoAcidResidue -->

    <owl:Class rdf:about="#AminoAcidResidue">
        <rdfs:subClassOf rdf:resource="#Molecular"/>
        <dc:description
            >This class will be declared equivalent ot the amino acid class description imported</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#AminoAcidResidueCharacter -->

    <owl:Class rdf:about="#AminoAcidResidueCharacter">
        <rdfs:subClassOf rdf:resource="#MolecularCharacter"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Amino_Acid_Datum"/>
                <owl:someValuesFrom rdf:resource="#AminoAcidResidueStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Datum"/>
                <owl:allValuesFrom rdf:resource="#AminoAcidResidueStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#AminoAcidResidueStateDatum -->

    <owl:Class rdf:about="#AminoAcidResidueStateDatum">
        <rdfs:subClassOf rdf:resource="#MolecularStateDatum"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#AncestralNode -->

    <owl:Class rdf:about="#AncestralNode">
        <rdfs:subClassOf rdf:resource="#Node"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Descendant"/>
                <owl:onClass rdf:resource="#Node"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#part_of"/>
                <owl:onClass rdf:resource="#RootedTree"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#BifurcatingTree -->

    <owl:Class rdf:about="#BifurcatingTree">
        <owl:equivalentClass rdf:resource="#FullyResolvedTree"/>
        <rdfs:subClassOf rdf:resource="#Tree"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#Bifurcation -->

    <owl:Class rdf:about="#Bifurcation">
        <owl:equivalentClass rdf:resource="#Dichotomy"/>
        <rdfs:subClassOf rdf:resource="#AncestralNode"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Child"/>
                <owl:onClass rdf:resource="#Node"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="#Polytomy"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#Branch -->

    <owl:Class rdf:about="#Branch">
        <owl:equivalentClass rdf:resource="#Edge"/>
        <dc:description
            >&#39;Branch&#39; is the domain-specific synonym for an edge of a (Phylogenetic) Tree or Network.  Branches may have properties such as length and degree of support.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#CDAOAnnotation -->

    <owl:Class rdf:about="#CDAOAnnotation">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:comment
            >Its possible that this base class should be discarded and that annotations should inherit from an imported base class if one exists.</rdfs:comment>
        <dc:description
            >The base class of annotations in CDAO.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#Categorical -->

    <owl:Class rdf:about="#Categorical">
        <rdfs:subClassOf rdf:resource="#CharacterStateDomain"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#CategoricalCharacter -->

    <owl:Class rdf:about="#CategoricalCharacter">
        <rdfs:subClassOf rdf:resource="#Character"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#CategoricalStateDatum -->

    <owl:Class rdf:about="#CategoricalStateDatum">
        <rdfs:subClassOf rdf:resource="#CharacterStateDatum"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#Character -->

    <owl:Class rdf:about="#Character">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Datum"/>
                <owl:someValuesFrom rdf:resource="#CharacterStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment rdf:datatype="&xsd;string"
            >Traits shown to be relevant for phylogenetic classification</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#CharacterStateDataMatrix -->

    <owl:Class rdf:about="#CharacterStateDataMatrix">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Character"/>
                <owl:someValuesFrom rdf:resource="#Character"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_TU"/>
                <owl:someValuesFrom rdf:resource="#TU"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has"/>
                <owl:someValuesFrom rdf:resource="#CharacterStateDataMatrixAnnotation"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >A matrix of character-state data, typically containing observed data, though in some cases the states in the matrix might be simulated or hypothetical. Synonyms: character Data matrix, character-state matrix</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#CharacterStateDataMatrixAnnotation -->

    <owl:Class rdf:about="#CharacterStateDataMatrixAnnotation">
        <rdfs:subClassOf rdf:resource="#CDAOAnnotation"/>
        <dc:description
            >Meta-information associated with a character matrix, such as, for the case of a sequence alignment, the method of alignment.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#CharacterStateDatum -->

    <owl:Class rdf:about="#CharacterStateDatum">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#belongs_to_Character"/>
                <owl:onClass rdf:resource="#Character"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#belongs_to_TU"/>
                <owl:onClass rdf:resource="#TU"/>
                <owl:qualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description
            >The instance of a given character for a given TU.  Its state is an object property drawn from a particular character state domain, e.g., the state of an Amino_Acid_State_Datum is an object property drawn from the domain Amino_Acid.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#CharacterStateDomain -->

    <owl:Class rdf:about="#CharacterStateDomain">
        <dc:description
            >The universe of possible states for a particular type of character, e.g., the states of an Amino_Acid character come from the Amino_Acid domain.</dc:description>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#Clade -->

    <owl:Class rdf:about="#Clade">
        <owl:equivalentClass rdf:resource="#HolophyleticGroup"/>
        <rdfs:subClassOf rdf:resource="#Tree"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#CommonAncestralNode -->

    <owl:Class rdf:about="#CommonAncestralNode">
        <rdfs:subClassOf rdf:resource="#AncestralNode"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Descendant"/>
                <owl:onClass rdf:resource="#TerminalNode"/>
                <owl:minQualifiedCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minQualifiedCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#Compound -->

    <owl:Class rdf:about="#Compound">
        <rdfs:subClassOf rdf:resource="#Categorical"/>
    </owl:Class>
    


    <!-- http://www.evolutionaryontology.org/cdaov3.owl#CompoundCharacter -->

    <owl:Class rdf:about="#CompoundCharacter">
        <rdfs:subClassOf rdf:resource="#CategoricalCharacter"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Compound_Datum"/>
                <owl:someValuesFrom rdf:resource="#CompoundStateDatum"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#has_Character"/>
                <owl:someValuesFrom rdf:resource="#Character"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
